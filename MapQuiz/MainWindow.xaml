<Window x:Class="MapQuiz.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="450" Width="600" xmlns:my="clr-namespace:MapQuiz">
    <DockPanel>
        <Menu DockPanel.Dock="Top" Height="23" Name="menu1">
            <MenuItem Header="File">
                <MenuItem Header="Load" />
                <MenuItem Header="Save" />
                <Separator />
                <MenuItem Header="Quit" />
            </MenuItem>
            <MenuItem Header="Mode">
                <MenuItem Header="QuizMode" Click="Menu_QuizMode_Click" />
                <MenuItem Header="EditMode" Click="Menu_EditMode_Click" />
            </MenuItem>
        </Menu>
        <Grid DockPanel.Dock="Bottom" Name="MainGrid"></Grid>
    </DockPanel>
</Window>
